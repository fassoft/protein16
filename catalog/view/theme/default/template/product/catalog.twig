{{ header }}

{{ column_left }}{{ column_right }}
<div class="container">
<div class="row">
<div id="content" class="col-sm-12">{{ content_top }}

<h1>Каталог продукции</h1>

<div id="catalog">
  
{% for catalogi in catalog %}
<div>
      <div class="image"><a href="{{ catalogi.href }}"><img src="{{ catalogi.image }}" alt="{{ catalogi.name }}" border="1" title="{{ catalogi.name }}" /></a></div>
      <div class="catalog-name"><a href="{{ catalogi.href }}">{{ catalogi.name }}</a></div>
	<div class="children">
		<div class="catalog-name"><a href="{{ catalogi.href }}">{{ catalogi.name }}</a></div><BR>
		{% for childrens in catalogi %}	  
			<div class="name"><a href="{{ childrens.href }}">{{ childrens.name }}</a></div>	  
		{% endfor %}

	</div>	  
</div>
{% endfor %}
</div>

{{ content_bottom }}</div></div></div>
   

  
{{ footer }}